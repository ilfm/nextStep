package nextStepProjectStep2;

public class StringCalculator {

	public void stringCalculator(String str) {
		
		String pattern = "[\\,]";
		str = str.replaceAll(pattern, "");
		System.out.println("str===="+str);
		
	}
	public void cutomStringCalculator(String str) {
		
		//, : 구분자 분리
		// , 있는지 체크
		// 있으면  분리
		//: 있는지 체크 
		// 있으면 분리
		// 더하기 
		
	}
}
